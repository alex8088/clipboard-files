{
	"name": "clipboard-files",
	"version": "0.0.8",
	"description": "A nodejs addon, read file path from clipboard, support win32 and mac osx",
	"main": "./lib/index.js",
	"directories": {
		"lib": "lib",
		"test": "test"
	},
	"binary": {
		"module_name": "binding",
		"module_path": "./lib/binding/{node_abi}-{platform}-{arch}/",
		"package_name": "{node_abi}-{platform}-{arch}.tar.gz",
		"host": "https://github.com/alex8088/clipboard-files/releases/download/",
		"remote_path": "v{version}"
	},
	"keywords": [
		"clipboard",
		"files",
		"nwjs",
		"electron"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/alex8088/clipboard-files.git"
	},
	"scripts": {
		"test": "node ./test",
		"install": "node-pre-gyp install --fallback-to-build"
	},
	"author": "alex.wei",
	"license": "MIT",
	"gypfile": true,
	"dependencies": {
		"nan": "^2.13.2",
		"node-pre-gyp": "^0.12.0"
	}
}
